<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.fi.activity"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".LifeActivity">
        </activity>
        <activity android:name=".NormalActivity">
        </activity>
        <activity
            android:name=".DialogActivity"
            android:theme="@style/Theme.AppCompat.Dialog">
        </activity>
        <activity android:name=".ModelActivity">
        </activity>
        <activity android:name=".First_Model1_Activity">
        </activity>
        <activity
            android:name=".First_Model2_Activity"
            android:launchMode="singleTop">
        </activity>
        <activity android:name=".Second_Model2_Activity">
        </activity>
    </application>

</manifest>
